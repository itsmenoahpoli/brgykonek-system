  "description": "TypeScript backend application with Express.js, Mongoose, and Swagger documentation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "migrate": "ts-node src/migrations/index.ts",
    "migrate:rollback": "ts-node src/migrations/index.ts rollback"
  },
  "keywords": [
    "typescript",
    "express",
    "mongoose",
    "jwt",
    "swagger"
  ],
  "author": "",
  "license": "MIT",
